{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [
			"error",
			"never"
		],
        "indent": [
			"error",
			4
		],
        "no-console": 0
    }
}
